{
  "name": "@ishikawa-masashi/math",
  "version": "0.4.8",
  "main": "./dist/index.mjs",
  "types": "./types/index.d.ts",
  "scripts": {
    "clean": "rimraf dist types",
    "watch": "tsc -w | vite build --watch",
    "build": "yarn tsc && vite build",
    "build2": "yarn clean && tsc && vite build",
    "build:debug": "node --inspect-brk=5858 ./node_modules/.bin/vite build",
    "release:minor": "npm version minor && npm publish --access public",
    "release:patch": "npm version patch && npm publish --access public",
    "lint": "eslint --fix \"src/**/*.{js,ts}\"",
    "typecheck": "tsc -p . --noEmit --emitDeclarationOnly false",
    "test": "yarn vitest"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.1.4",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "babylonjs": "^6.3.1",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.5",
    "vitest": "^0.31.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ishikawa-Masashi"
  },
  "files": [
    "*"
  ]
}
